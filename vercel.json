{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.go"
    }
  ],
  "env": {
    "PORT": "8080",
    "STREMTHRU_DATABASE_URI": "sqlite://./data/stremthru.db",
    "STREMTHRU_REDIS_URI": "your_redis_uri"
  },
  "ignore": ["test/*", "docs/*"]
}
